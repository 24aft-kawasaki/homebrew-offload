{
    "customizations": {
        "vscode": {
            "settings": {
                "workbench.colorTheme": "Default Dark+"
            },
            "extensions": [
                "gxl.git-graph-3",
                "ms-python.python"
            ]
        }
    },
    "postCreateCommand": {
        // Array: Passed to the OS for execution without going through a shell
        // String: Goes through a shell
        // Object: Object types to allow for parallel execution
        "install-requirements": "make --directory=${containerWorkspaceFolder}/.devcontainer install-brew install-python put-brew-offload-config-file"
    }
}